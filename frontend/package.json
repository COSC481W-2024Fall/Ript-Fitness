{
  "name": "ript",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "node setup-env.js && npx expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.0",
    "@expo/vector-icons": "^14.0.2",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/drawer": "^6.7.2",
    "@react-navigation/material-top-tabs": "^6.6.14",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@react-navigation/stack": "^6.4.1",
    "@shopify/react-native-skia": "1.5.0",
    "expo-font": "~13.0.1",
    "expo-haptics": "~14.0.0",
    "expo-network": "~7.0.0",
    "expo-status-bar": "~2.0.0",
    "install": "^0.13.0",
    "jest-fetch-mock": "^3.0.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.2",
    "react-native-dotenv": "^3.4.11",
    "react-native-draggable-flatlist": "^4.0.1",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-pager-view": "6.4.1",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.1.0",
    "react-native-svg": "15.8.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "~0.19.13",
    "victory": "^37.3.0",
    "victory-native": "^36.3.1"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect",
      "./jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(victory(-.*)?|react-native-svg|d3-(array|interpolate|format|time|time-format|scale|shape)|internmap|@expo|expo-font|expo-modules-core|react-native|@react-navigation|react-native-reanimated|@react-native|@testing-library)/)"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "\\.svg$": "react-native-svg-mock"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.25.7",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@testing-library/jest-native": "^5.3.0",
    "@testing-library/react-native": "^12.6.0",
    "@types/expo": "^32.0.13",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.3.12",
    "@types/react-native": "^0.72.8",
    "@types/react-native-vector-icons": "^6.4.18",
    "babel-jest": "^29.7.0",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "expo": "^52.0.7",
    "jest": "~29.7.0",
    "jest-expo": "^52.0.0",
    "react-native-svg-mock": "^2.0.0",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.3.3"
  },
  "private": true
}
